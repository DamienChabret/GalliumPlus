<Page x:Class="Couche_IHM.Frames.FrameComptes"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Couche_IHM.Frames"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FrameComptes"
      Background="White">

    <Grid>
        <TextBlock Text="Comptes" />
        <ListView x:Name="listUser" FontSize="10" Style="{StaticResource onsenfou}">
            <ListView.View>
                <GridView x:Name="GridViewStockControl">
                    <GridViewColumn HeaderContainerStyle="{StaticResource GridViewColumnHeaderStyle1}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Page>
